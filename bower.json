  "license": "MIT",
  "homepage": "none",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "install": {
    "path": {
      "css": "static/css",
      "js": "static/js",
      "map": "static/js",
      "woff": "static/fonts"
    },
    "sources": {
      "jquery": [
        "public/bower_components/jquery/dist/jquery.min.js",
        "public/bower_components/jquery/dist/jquery.min.map"
      ],
      "ol3": [
        "public/bower_components/ol3/ol.js",
        "public/bower_components/ol3/ol-debug.js",
        "public/bower_components/ol3/ol.css"
      ],
      "proj4": [
        "public/bower_components/proj4/dist/proj4.js"
      ],
      "d3": [
        "public/bower_components/d3/d3.min.js"
      ],
      "bootstrap": [
        "public/bower_components/bootstrap/dist/css/bootstrap.min.css",
        "public/bower_components/bootstrap/dist/js/bootstrap.min.js",
        "public/bower_components/bootstrap/fonts/glyphicons-halflings-regular.woff"
      ],
      "ladda": [
        "public/bower_components/ladda/dist/ladda.jquery.min.js",
        "public/bower_components/ladda/dist/ladda.min.css",
        "public/bower_components/ladda/dist/ladda.min.js",
        "public/bower_components/ladda/dist/ladda-themeless.min.css",
        "public/bower_components/ladda/dist/spin.min.js"
      ]
    }
  },
  "dependencies": {
    "jquery": "1.11.2",
    "ol3": "ol3-bower#3.14.2",
    "d3": "3.5.16",
    "jquery-mobile": "1.4.5",
    "proj4": "2.3.14",
    "bootstrap": "3.3.6",
    "ladda": "ladda-boostrap-hakimel#1.0.0"
